{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node",
      "bun-types",
      "src/**/*.ts", "src/**/*.d.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
  }
}